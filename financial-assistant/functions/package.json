    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "eslint \"src/**/*\""
  },
  "engines": {
    "node": "18"
  },
  "keywords": [
    "firebase",
    "functions",
    "financial-assistant",
    "ai"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.2.0",
    "axios": "^1.6.0",
    "firebase-admin": "^11.11.0",
    "firebase-functions": "^4.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.10",
    "typescript": "^5.2.2"
  }
}
